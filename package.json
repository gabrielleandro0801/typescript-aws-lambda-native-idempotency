    "ts-node-dev": "2.0.0",
    "typescript": "5.4.5"
  },
  "dependencies": {
    "@aws-lambda-powertools/idempotency": "2.1.0",
    "@aws-sdk/client-dynamodb": "3.565.0",
    "@aws-sdk/client-sqs": "3.565.0",
    "@aws-sdk/lib-dynamodb": "3.565.0",
